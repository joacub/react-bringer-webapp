  "main": "bin/server.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "cross-env HOST=127.0.0.1 NODE_PATH=src NODE_ENV=production PORT=9777 APIPORT=5757 node --trace-deprecation ./bin/server.js",
    "start-prod-api": "cross-env HOST=127.0.0.1 NODE_PATH=api NODE_ENV=production APIPORT=5757 node --trace-deprecation ./bin/api.js",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "start-dev": "cross-env HOST=127.0.0.1 TZ=UTC NODE_PATH=src NODE_ENV=development PORT=3000 APIPORT=3737 node --trace-deprecation ./bin/server.js",
    "start-dev-api": "cross-env HOST=127.0.0.1 TZ=UTC NODE_PATH=api NODE_ENV=development APIPORT=3737 node --trace-deprecation ./bin/api.js",
    "watch-client": "cross-env UV_THREADPOOL_SIZE=100 NODE_PATH=src NODE_ENV=development PORT=3000 APIPORT=3737 node --trace-deprecation webpack/webpack-dev-server.js",
    "build": "cross-env NODE_ENV=production webpack-cli --colors --display-error-details --config=webpack/prod.config.js",
    "build-profile": "cross-env NODE_ENV=production webpack --colors --display-error-details --config=webpack/prod.config.js --profile --json > stats.json",
    "postinstall": "concurrently \"npm run build\"",
    "precommit": "lint-staged",
    "prettier": "prettier-eslint --write --eslint-config-path .eslintrc './+(src|api)/**/*.js'",
    "lint": "eslint -c .eslintrc api src",
    "test": "cross-env NODE_PATH=src jest src",
    "test-node": "cross-env NODE_PATH=api jest api",
    "lighthouse-report": "lighthouse http://127.0.0.1:9777 --view",
    "docker-build": "docker-compose build",
    "docker-dev": "cross-env NGINX_HOST=127.0.0.1 NGINX_PORT=3000 PORT=3000 docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --no-deps",
    "docker-up": "cross-env NGINX_HOST=127.0.0.1 NGINX_PORT=80 PORT=9777 docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --no-deps -d",
    "docker-down": "docker-compose down",
    "sitemap-builder-dev": "cross-env NODE_PATH=api NODE_ENV=development APIPORT=3000 node ./bin/sitemapBuilder.js",
    "sitemap-builder": "cross-env NODE_PATH=api NODE_ENV=production APIPORT=3737 node ./bin/sitemapBuilder.js",
    "training-data-dev": "cross-env NODE_PATH=api NODE_ENV=development APIPORT=3000 node ./bin/trainingData.js",
    "training-data-prod": "cross-env NODE_PATH=api NODE_ENV=production APIPORT=3737 node ./bin/trainingData.js",
    "training-ia-dev": "cross-env NODE_PATH=api NODE_ENV=development APIPORT=3000 node ./bin/training.js",
    "training-ia-prod": "cross-env NODE_PATH=api NODE_ENV=production APIPORT=3737 node ./bin/training.js",
    "draft-exporter-dev": "cross-env NODE_PATH=api NODE_ENV=development APIPORT=3000 node ./bin/exportToDraft.js",
    "draft-exporter-builder": "cross-env NODE_PATH=api NODE_ENV=production APIPORT=3737 node ./bin/exportToDraft.js",
    "utils-dev": "cross-env NODE_PATH=api NODE_ENV=development APIPORT=3000 node ./bin/utils.js",
    "utils": "cross-env NODE_PATH=api NODE_ENV=production APIPORT=3737 node ./bin/utils.js",
    "googleplus-dev": "cross-env NODE_PATH=api NODE_ENV=development APIPORT=3000 node --max-old-space-size=8192 ./bin/googlePlus.js ",
    "googleplus": "cross-env NODE_PATH=api NODE_ENV=production APIPORT=3737 node --max-old-space-size=8192 ./bin/googlePlus.js",
    "installdb-dev": "cross-env NODE_PATH=api NODE_ENV=development APIPORT=3737 node ./bin/installDb.js",
    "installdb-prod": "cross-env NODE_PATH=api NODE_ENV=production APIPORT=3737 node ./bin/installDb.js",
    "twitter-bot-dev": "cross-env NODE_PATH=api NODE_ENV=development APIPORT=3737 node ./bin/TwitterBot.js",
    "twitter-bot-prod": "cross-env NODE_PATH=api NODE_ENV=production APIPORT=3737 node ./bin/TwitterBot.js"
  },
  "lint-staged": {
    "+(src|api)/**/*.js": [
      "prettier-eslint --write --eslint-config-path .eslintrc",
      "eslint -c .eslintrc",
      "git add"
    ]
  },
